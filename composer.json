{
    "name": "kakifrucht/flarum-de",
    "type": "flarum-extension",
    "description": "German language pack for Flarum and its bundled extensions.",
    "homepage": "https://github.com/Kakifrucht/flarum-de",
    "keywords": ["locale"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Kakifrucht/flarum-de/issues",
        "source": "https://github.com/Kakifrucht/flarum-de",
        "forum": "https://discuss.flarum.org/d/2648"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.15"
    },
    "extra": {
        "flarum-extension": {
            "title": "German",
            "category": "language",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#ffffff",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "de",
            "title": "Deutsch"
        }
    }
}
